type 'SomeMap' dictionary('string', 'bool')

type "Entity" message {
  a 'string'
  b int
  c long
  d 'bool'
  e 'float'
  f 'SomeMap'
}

service {
  name "Test service $providedVar"

  get "/profile" spec {
    response 'Entity'
  }

  delete '/profile/{id}' spec {
    name 'Delete profile'
  }

}
